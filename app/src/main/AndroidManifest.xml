<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.noveo.internship.storage"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".DatabaseLabApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:fullBackupContent="false">
        <activity
            android:name="com.noveo.internship.storage.ui.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.noveo.internship.storage.ui.activity.orm.UserActivity"/>

        <activity android:name="com.noveo.internship.storage.ui.activity.orm.EmailActivity"/>
    </application>

</manifest>
